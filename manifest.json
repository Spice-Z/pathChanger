{
  "name": "PathChanger",
  "description": "create local path for mac and windows",
  "version": "1.0",
  "browser_action": {
    "default_popup": "changer.html",
    "default_icon": "p.png"
  },
  "background": {
    "scripts": [
      "function.js"
    ]
  },
  "permissions": [
    "clipboardRead"
  ],
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens changer.html"
    }
  }
}
